{
  "name": "simple-shell",
  "description": "Personal website or portfolio boilerplate.",
  "version": "1.1.2",
  "main": "src/index.tsx",
  "repository": "https://github.com/moquette/react-typescript-starter.git",
  "keywords": [
    "webpack",
    "eslint",
    "prettier",
    "react-router-dom",
    "headless-ui"
  ],
  "license": "MIT",
  "scripts": {
    "start": "webpack-cli serve --mode=development --env development",
    "build": "yarn typecheck && webpack --mode=production --env production --progress",
    "format": "prettier './src/**/*.{js,jsx,ts,tsx,json,scss,css,md,html}' --write",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --quiet --fix",
    "typecheck": "tsc --noEmit",
    "prep": "yarn format && yarn lint && yarn typecheck",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "emailjs-com": "^3.2.0",
